{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "cloudwatch:PutMetricData"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "lambda:InvokeFunction"
      ],
      "Resource": "arn:aws:lambda:us-east-1:861229788715:function:dcp-matrix-service-driver-predev"
    },
    {
      "Effect": "Allow",
      "Action": [
        "dynamodb:GetItem"
      ],
      "Resource": [
        "arn:aws:dynamodb:us-east-1:861229788715:table/dcp-matrix-service-state-table-predev",
        "arn:aws:dynamodb:us-east-1:861229788715:table/dcp-matrix-service-output-table-predev",
        "arn:aws:dynamodb:us-east-1:861229788715:table/dcp-matrix-service-cache-table-predev"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "dynamodb:PutItem"
      ],
      "Resource": [
        "arn:aws:dynamodb:us-east-1:861229788715:table/dcp-matrix-service-cache-table-predev"
      ]
    }
  ]
}
